<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane minHeight="600.0" minWidth="800.0" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="monoko.ui.MainMenuController">
  <bottom>
    <VBox id="VBox" alignment="CENTER" minHeight="500.0" minWidth="800.0" prefHeight="-1.0" prefWidth="-1.0" spacing="25.0">
      <children>
        <Button fx:id="_playButton" mnemonicParsing="false" onAction="#onPlayButtonClicked" prefHeight="35.0" prefWidth="200.0" text="Play" />
        <Button fx:id="_teamEditorButton" mnemonicParsing="false" onAction="#onTeamEditorButtonClicked" prefHeight="35.0" prefWidth="200.0" text="Team Editor" />
        <Button fx:id="_helpButton" mnemonicParsing="false" onAction="#onHelpButtonClicked" prefHeight="35.0" prefWidth="200.0" text="Help" />
        <Button fx:id="_loreButton" mnemonicParsing="false" onAction="#onLoreButtonClicked" prefHeight="35.0" prefWidth="200.0" text="Lore" />
        <Button fx:id="_quitButton" mnemonicParsing="false" onAction="#onQuitButtonClicked" prefHeight="35.0" prefWidth="200.0" text="Quit" />
      </children>
      <BorderPane.margin>
        <Insets />
      </BorderPane.margin>
    </VBox>
  </bottom>
  <top>
    <Label id="titleLabel" fx:id="_titleLabel" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="100.0" minWidth="800.0" prefHeight="-1.0" prefWidth="-1.0" text="Monoko" />
  </top>
</BorderPane>
