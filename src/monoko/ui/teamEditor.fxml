<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="_rootHBox" alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <VBox alignment="CENTER" maxHeight="-1.0" maxWidth="960.0" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" HBox.hgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" minHeight="50.0" minWidth="200.0" text="Characters">
          <font>
            <Font size="24.0" fx:id="x1" />
          </font>
        </Label>
        <Separator prefWidth="-1.0" />
        <HBox id="HBox" alignment="TOP_LEFT" spacing="5.0" VBox.vgrow="ALWAYS">
          <children>
            <Pane minWidth="5.0" prefHeight="-1.0" prefWidth="-1.0" />
            <FlowPane fx:id="_charactersFlowpane" hgap="10.0" maxHeight="540.0" maxWidth="540.0" orientation="HORIZONTAL" prefHeight="-1.0" prefWidth="-1.0" vgap="10.0" />
            <Pane minWidth="5.0" prefHeight="-1.0" prefWidth="-1.0" />
          </children>
          <VBox.margin>
            <Insets />
          </VBox.margin>
        </HBox>
        <Separator prefWidth="-1.0" />
        <Button fx:id="_createCharacterButton" font="$x1" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" onAction="#onCreateCharacterClicked" text="Create Character" />
        <Pane minHeight="10.0" prefHeight="-1.0" prefWidth="-1.0" />
      </children>
    </VBox>
    <Separator maxHeight="950.0" orientation="VERTICAL" prefHeight="-1.0" />
    <VBox alignment="CENTER" maxHeight="-1.0" maxWidth="960.0" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" HBox.hgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" font="$x1" minHeight="50.0" minWidth="200.0" text="Teams" />
        <Separator prefWidth="-1.0" />
        <VBox fx:id="_teamsVBox" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
        <Separator prefWidth="-1.0" />
        <Button fx:id="_createTeamButton" font="$x1" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" onAction="#onCreateTeamClicked" text="Create Team" />
        <Pane minHeight="10.0" prefHeight="-1.0" prefWidth="-1.0" />
      </children>
    </VBox>
  </children>
</HBox>
