<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="_rootVBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="720.0" minWidth="1280.0" prefHeight="-1.0" prefWidth="-1.0" style="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <HBox alignment="TOP_CENTER" maxHeight="100.0" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label fx:id="_titleLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="300.0" text="Lobby">
          <font>
            <Font size="24.0" fx:id="x1" />
          </font>
        </Label>
      </children>
    </HBox>
    <HBox alignment="CENTER" maxHeight="800.0" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Pane minWidth="10.0" prefHeight="-1.0" prefWidth="-1.0" />
        <TitledPane fx:id="_playersTitledPane" alignment="CENTER" animated="false" collapsible="false" content="$null" contentDisplay="RIGHT" maxHeight="750.0" maxWidth="550.0" text="Connected Players" HBox.hgrow="ALWAYS">
          <font>
            <Font size="20.0" fx:id="x2" />
          </font>
        </TitledPane>
        <Pane minWidth="10.0" prefHeight="-1.0" prefWidth="-1.0" />
        <TitledPane fx:id="_invitationsTitledPane" alignment="CENTER" animated="false" collapsible="false" content="$null" contentDisplay="RIGHT" font="$x2" maxHeight="750.0" maxWidth="550.0" text="Game Invitations" HBox.hgrow="ALWAYS" />
        <Pane minWidth="10.0" prefHeight="-1.0" prefWidth="-1.0" />
        <VBox alignment="TOP_CENTER" minWidth="100.0" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0" HBox.hgrow="ALWAYS">
          <children>
            <Label alignment="CENTER" font="$x2" maxWidth="1.7976931348623157E308" text="Choose your Team : " />
            <ComboBox fx:id="_teamsComboBox" maxWidth="200.0" VBox.vgrow="ALWAYS">
              <items>
                <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Item 1" />
                  <String fx:value="Item 2" />
                  <String fx:value="Item 3" />
                </FXCollections>
              </items>
              <VBox.margin>
                <Insets />
              </VBox.margin>
            </ComboBox>
            <Button fx:id="_mainMenuButton" font="$x1" minHeight="50.0" mnemonicParsing="false" onAction="#onMainMenuClicked" text="Main Menu" />
          </children>
        </VBox>
      </children>
    </HBox>
  </children>
</VBox>
